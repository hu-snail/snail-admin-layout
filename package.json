{
  "name": "@snailadmin/ui",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@7.3.0",
  "description": "Use tsx syntax to encapsulate element-plus Layout",
  "keywords": [
    "@snailadmin/layout",
    "element-plus",
    "typescript",
    "vue3",
    "vite",
    "tsx"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "commit": "git-cz",
    "build": "npm run build:components",
    "build-all": "vite build",
    "build:components": "esno ./build/index.ts && vue-tsc -p tsconfig.build.json",
    "lint": "prettier --write  \"packages/**/*.{ts,tsx,vue}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx,vue}": [
      "lint --fix",
      "git add"
    ]
  },
  "homepage": "https://github.com/hu-snail/snail-admin-layout/tree/main/#readme",
  "bugs": {
    "url": "https://github.com/hu-snail/snail-admin-layout/issues"
  },
  "license": "MIT",
  "author": "hu-snail",
  "repository": {
    "type": "git",
    "url": "https://github.com/hu-snail/snail-admin-layout"
  },
  "main": "@snailadmin/ui.umd.js",
  "module": "@snailadmin/ui.mjs",
  "types": "dist/components/index.d.ts",
  "unpkg": "dist/snail-admin.umd.js",
  "jsdelivr": "dist/snail-admin.umd.js",
  "exports": {
    ".": {
      "require": "./dist/snail-admin.umd.js",
      "import": "./dist/snail-admin.mjs",
      "types": "./dist/components/index.d.ts"
    }
  },
  "sideEffects": false,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "peerDependencies": {
    "element-plus": "^2.0.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@iconify-json/icon-park-outline": "^1.1.9",
    "animate.css": "^4.1.1",
    "element-plus": "^2.2.28",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@esbuild-kit/cjs-loader": "^2.2.0",
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "commitizen": "^4.2.4",
    "commitlint": "^17.0.3",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.7.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.3",
    "unplugin-icons": "^0.15.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}